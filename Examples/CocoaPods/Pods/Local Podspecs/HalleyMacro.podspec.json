  "homepage": "https://github.com/Infinum/Halley",
  "license": {
    "type": "MIT",
    "file": "LICENSE"
  },
  "authors": {
    "Infinum": "ios@infinum.hr",
    "Filip Gulan": "filip.gulan@infinum.com",
